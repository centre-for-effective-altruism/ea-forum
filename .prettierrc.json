{
  "plugins": ["prettier-plugin-packagejson"],
  "proseWrap": "always",
  "printWidth": 85,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "singleAttributePerLine": false,
  "endOfLine": "lf"
}
